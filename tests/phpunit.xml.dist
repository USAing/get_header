<?xml version="1.0" encoding="UTF-8"?>
<phpunit bootstrap="./bootstrap.php" colors="true">
    <testsuites>
        <testsuite name="get_header Test Suite">
            <directory>./get_headerTest/</directory>
        </testsuite>
    </testsuites>
    <filter>
        <whitelist>
            <directory suffix=".php">../src/get_header</directory>
            <file>../src/get_header</file>
            <exclude>
                <directory>../vendor</directory>
                <file>../vendor</file>
            </exclude>
        </whitelist>
    </filter>
</phpunit>